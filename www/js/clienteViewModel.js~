'use strict';

var ClientView Model = function (){
	
	var self = this;

	self.name = ko.observable();
	self.lastname1 = ko.observable();
	self.lastname2 = ko.observable();
	self.phone = ko.observable();
	self.mail = ko.observable();
	self.password = ko.observable();

	self.Create = function () {
		
		var url = 'http://etickettest-mespinozas.rhcloud.com/api/clients';
		var formData = {name:self.name,lname1:self.lastname1,lname2:self.lastname2,phone:self.phone,password:self.password,mail:self.mail}
		$.ajax({

			  url: 	url,
			  type: 	'POST',
			  dataType: 'json',
			  contentType: 'application/json; charset=utf-8',
			  data: formData,
			  success: function(data){
				console.log(data);
				self.productList(data);
			  },
			  error: function(xhr, type){
				console.error(xhr);
				console.error(type);
			  }
		});
	};
	self.save = function(){
    		alert('hello');
  	};
};
